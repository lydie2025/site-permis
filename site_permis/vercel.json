{
  "version": 2,
  "rewrites": [
    {
      "source": "/HeroImage/(.*)",
      "destination": "/HeroImage/$1"
    },
    {
      "source": "/logo.jpg",
      "destination": "/logo.jpg"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/HeroImage/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/logo.jpg",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
